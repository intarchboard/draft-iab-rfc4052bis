      "number": 17,
      "id": "PR_kwDOMUjy5c6L6U_C",
      "title": "Update draft-krishnan-iab-rfc4052bis.md",
      "url": "https://github.com/intarchboard/draft-iab-rfc4052bis/pull/17",
      "state": "MERGED",
      "author": "sureshkrishnan",
      "authorAssociation": "CONTRIBUTOR",
      "assignees": [],
      "labels": [],
      "body": "Explained that IETF processes and documents are open to everyone and the liaison relationship is usually required from the other organization",
      "createdAt": "2025-02-20T14:51:52Z",
      "updatedAt": "2025-02-20T15:40:21Z",
      "baseRepository": "intarchboard/draft-iab-rfc4052bis",
      "baseRefName": "main",
